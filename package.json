{
  "name": "ngx-triple-ssp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ng-triple-exemple",
    "watch": "ng build --watch --configuration development",
    "build:wba": "ng build --configuration production --aot --base-href / --stats-json",
    "build:analyzer": "npx webpack-bundle-analyzer dist/opcoes/stats.json",
    "test": "ng test",
    "test:docker": "ng test --watch=false --sourceMap=false --codeCoverage=true --browsers ChromeHeadless",
    "test:common": "ng test --browsers Chrome,Firefox",
    "test:ci": "ng test --watch=true --reporters junit --browsers ChromeHeadless",
    "test:cov": "ng test --watch=true --sourceMap=true --codeCoverage=true --browsers ChromeHeadless",
    "lint": "ng lint",
    "eslint:init": "eslint --init",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0",
    "bootstrap": "^5",
    "jquery": "^3.6.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.13",
    "@angular/cli": "^16.2.13",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.2.0",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.7.1",
    "puppeteer": "^13.5.2",
    "typescript": "~5.1.3",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
